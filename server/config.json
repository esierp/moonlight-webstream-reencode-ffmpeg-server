{
  "web_server": {
    "bind_address": "0.0.0.0:8080",
    "certificate": {
      "private_key_pem": "server/key.pem",
      "certificate_pem": "server/cert.pem"
    },
    "url_path_prefix": "",
    "session_cookie_secure": true,
    "session_cookie_expiration": {
      "secs": 86400,
      "nanos": 0
    },
    "first_login_create_admin": true,
    "first_login_assign_global_hosts": true,
    "default_user_id": 1246126999,
    "forwarded_header": null
  },
  "webrtc": {
    "ice_servers": [],
    "port_range": {
      "min": 40000,
      "max": 40100
    },
    "nat_1to1": {
      "ips": ["192.168.188.102"],
      "ice_candidate_type": "host"
    },
    "network_types": ["udp4"],
    "include_loopback_candidates": true
  },
  "video": {
    "server_decode": true,
    "ffmpeg": {
      "encoder": "libx264",
      "preset": "veryfast",
      "bitrate_kbps": 12000,
      "fps": 60
    }
  },
  "default_settings": {
    "serverReencodeEnabled": true,
    "serverReencodeCodec": "h264",
    "serverReencodeBitrateKbps": 12000
  }
}
